<?xml version="1.0"?>
<package >
  <metadata>
    <id>IPEIT.TemplateResolver.Start</id>
    <version>1.0.0</version>
    <authors>sultan.imbayev</authors>
    <owners>IPEIT KBTU</owners>
    <projectUrl>https://github.com/sultanimbayev/IEIT.TemplateResolver</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>
		Library for working with template files and getting their full paths everywhere in the code.
		This is the quick start package that transforms your configuration file and creates a folder for your templates.
	</description>
    <releaseNotes>
		First release
	</releaseNotes>
    <tags>FileSystem Templates Files</tags>
	<dependencies>
		<dependency id="IPEIT.TemplateResolver" version="1.0.0"/>
	</dependencies>
	<frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Configuration" />
	</frameworkAssemblies>
  </metadata>
  <files>
	<file src="..\IEIT.TemplateResolver\content\**" target="content" />
  </files>
</package>